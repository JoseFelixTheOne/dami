package com.dawi.models;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;
import jakarta.persistence.Table;
import lombok.Data;

@Entity
@Table(name = "tb_menupermisos")
@Data
public class MenuPermisos {
	@Id
	private int id_menupermisos;
	
	@ManyToOne
	@JoinColumn(name = "id_tipousu",insertable = false,updatable = false)
	private int id_tipousu;

	@ManyToOne
	@JoinColumn(name = "id_menu",insertable = false,updatable = false)
	private int id_menu;
	
	private int activo_menupermisos;

}
